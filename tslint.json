{
  "rules": {
    "class-name": false,
    "curly": false,
    "eofline": false,
    "indent": [
      false,
      "spaces"
    ],
    "max-line-length": [
      false,
      500
    ],
    "member-ordering": [
      false,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-arg": false,
    "no-construct": false,
    "no-duplicate-key": false,
    "no-duplicate-variable": false,
    "no-empty": false,
    "no-eval": false,
    "trailing-comma": false,
    "no-trailing-whitespace": false,
    "no-unused-expression": false,
    "no-unused-variable": false,
    "no-unreachable": false,
    "no-use-before-declare": false,
    "one-line": [
      false,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": [
      false,
      "single"
    ],
    "semicolon": false,
    "triple-equals": [
      false,
      "allow-null-check"
    ],
    "variable-name": false,
    "whitespace": [
      false,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}
