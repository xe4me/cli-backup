    "http-proxy-middleware": "^0.17.2",
    "husky": "^0.11.9",
    "ignore-loader": "^0.1.1",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
