    "style-loader": "^0.13.1",
    "ts-helpers": "1.1.1",
    "ts-node": "^0.9.0",
    "tslint": "^3.7.1",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.4.2",
    "typescript": "^2.0.0-beta",
